{
  "name": "myquota-webstore",
  "version": "1.0.0",
  "description": "Aplikasi webstore untuk penjualan paket internet dengan panel admin",
  "main": "index.html",
  "scripts": {
    "start": "serve -s . -l 3000",
    "build": "echo 'No build process needed for static site'",
    "dev": "live-server --port=3000 --open=index.html",
    "admin": "live-server --port=3001 --open=admin.html",
    "test": "jest",
    "lint": "eslint *.js",
    "format": "prettier --write *.{html,css,js}",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "webstore",
    "paket-internet",
    "xl",
    "axis",
    "telkomsel",
    "admin-panel",
    "google-sheets"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://your-website.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/myquota-webstore.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/myquota-webstore/issues"
  },
  "homepage": "https://your-domain.vercel.app",
  "devDependencies": {
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "live-server": "^1.2.2",
    "prettier": "^2.8.0",
    "serve": "^14.0.0"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "index.html",
    "admin.html",
    "style.css",
    "admin-style.css",
    "script.js",
    "admin-script.js",
    "config.js",
    "manifest.json",
    "sw.js",
    "README.md"
  ]
}
